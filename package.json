{
  "name": "sequelize-models",
  "version": "2.0.0-alpha.1",
  "description": "Node.js SequelizeJS ORM model command line and load utilities",
  "homepage": "https://github.com/gbahamondezc/sequelize-models",
  "author": {
    "name": "Gonzalo Bahamondez",
    "email": "gonzalo.bahamondez.c@gmail.com",
    "url": "https://github.com/gbahamondezc"
  },
  "files": [
    "src"
  ],
  "main": "lib/sequelize-models.js",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "keywords": [
    "sequelize",
    "sequelizeJS",
    "models",
    "utilities"
  ],
  "devDependencies": {
    "ava": "^0.22.0",
    "dotenv": "^4.0.0",
    "jsdoc": "^3.5.4",
    "minami": "^1.2.3",
    "mysql2": "^1.4.1",
    "nyc": "^11.1.0",
    "pg": "^7.2.0",
    "pg-hstore": "^2.3.2",
    "run-sequence": "^2.1.0",
    "sequelize-cli": "^2.8.0",
    "sqlite3": "^3.1.9",
    "tedious": "^2.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gbahamondezc/sequelize-models.git"
  },
  "scripts": {
    "test": "ava --verbose",
    "test:watch": "ava --verbose -w",
    "test:docker": "docker-compose up test",
    "build": "docker-compose build",
    "migration:create": "sequelize migration:create --config config/postgres/migrations.js",
    "migrate:pg": "LOCAL=true sequelize db:migrate --config config/postgres/migrations.js",
    "migrate:pg:undo": "LOCAL=true sequelize db:migrate:undo --config config/postgres/migrations.js",
    "migrate:pg:docker": "sequelize db:migrate --config config/postgres/migrations.js",
    "migrate:my": "LOCAL=true sequelize db:migrate --config config/mysql/migrations.js",
    "migrate:my:undo": "LOCAL=true sequelize db:migrate:undo --config config/mysql/migrations.js",
    "migrate:my:docker": "sequelize db:migrate --config config/mysql/migrations.js",
    "stop:dev": "docker-compose stop"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "co": "^4.6.0",
    "debug": "^3.0.1",
    "lodash": "^4.17.4",
    "readdir-plus": "^1.0.0",
    "sequelize": "^4.7.5"
  }
}
